function main(): i32 {
    let numbers: [i32];
    numbers.push(5);
    numbers.push(10);
    numbers.push(15);

    print(f"Array length: {numbers.length}");

    let last: i32 = numbers.pop();
    print(f"Popped: {last}");

    let text: string = "a,b,c,d,e";
    let parts: [string] = text.split(",");

    print(f"Split into parts");

    let back: string = parts.join("-");
    print(f"Joined: {back}");

    return numbers.length;
}
