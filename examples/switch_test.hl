function test_switch(x: i32): i32 {
    let result: i32 = 0;

    switch (x) {
        case 1:
            result = 10;
            break;
        case 2:
            result = 20;
            break;
        case 3:
            result = 30;
            break;
        default:
            result = 99;
            break;
    }

    return result;
}

function main(): i32 {
    let a: i32 = test_switch(1);
    let b: i32 = test_switch(2);
    let c: i32 = test_switch(3);
    let d: i32 = test_switch(5);

    return a + b + c + d;
}
