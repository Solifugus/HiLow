function add(a: i32, b: i32): i32 {
    return a + b;
}

function multiply(a: i32, b: i32): i32 {
    return a * b;
}

function print_num(x: i32, dummy: i32): i32 {
    print(f"  Item: {x}");
    return 0;
}

function main(): i32 {
    let nums: [i32];
    nums.push(1);
    nums.push(2);
    nums.push(3);
    nums.push(4);
    nums.push(5);

    print("=== forEach ===");
    nums.forEach(print_num);

    print("=== reduce (sum) ===");
    let sum: i32 = nums.reduce(add, 0);
    print(f"Sum: {sum}");

    print("=== reduce (product) ===");
    let product: i32 = nums.reduce(multiply, 1);
    print(f"Product: {product}");

    return sum;
}
