function main(): i32 {
    let text: string = "  Hello World  ";

    print(f"Original: '{text}'");

    let trimmed: string = text.trim();
    print(f"Trimmed: '{trimmed}'");

    let upper: string = trimmed.toUpperCase();
    print(f"Upper: '{upper}'");

    let lower: string = upper.toLowerCase();
    print(f"Lower: '{lower}'");

    let len: i32 = trimmed.length;

    return len;
}
