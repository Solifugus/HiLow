function main(): i32 {
    print("=== Operator Test ===");

    let a: i32 = 10;
    let b: i32 = 5;

    let add: i32 = a + b;
    let sub: i32 = a - b;
    let mul: i32 = a * b;
    let div: i32 = a / b;
    let mod: i32 = a % b;

    print("Arithmetic operators work");

    let eq: bool = a ?= 10;
    let neq: bool = a != 5;
    let lt: bool = a < 20;
    let gt: bool = a > 5;
    let lte: bool = a <= 10;
    let gte: bool = a >= 10;

    print("Comparison operators work");

    let and_result: bool = true and false;
    let or_result: bool = true or false;
    let not_result: bool = not false;

    print("Logical operators work");

    let bitand: i32 = 12 & 10;
    let bitor: i32 = 12 | 10;
    let bitxor: i32 = 12 ^ 10;
    let bitnot: i32 = ~10;
    let shl: i32 = 1 << 3;
    let shr: i32 = 16 >> 2;

    print("Bitwise operators work");

    let x: i32 = 10;
    x += 5;
    x -= 3;
    x *= 2;
    x /= 4;
    x %= 5;

    print("Compound assignment operators work");

    print("All operators test complete!");

    return 0;
}
