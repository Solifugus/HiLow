function divide(a: i32, b: i32): i32 {
    if (b ??= 0) {
        return 0;
    }
    return a / b;
}

function main(): i32 {
    print("Testing nothing and unknown");

    let x: i32 = nothing;
    if (x ??= nothing) {
        print("x is nothing");
    }

    let result: i32 = divide(10, 2);
    print(f"10 / 2 = {result}");

    let result2: i32 = divide(10, 0);
    print(f"10 / 0 = {result2}");

    return result;
}
