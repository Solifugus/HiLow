function expensive(): i32 {
    print("This should not print!");
    return 42;
}

function main(): i32 {
    let x: i32 = 0;
    let result: i32 = 0;

    if (x ?= 0 or expensive() ?= 42) {
        result = 1;
    }

    let y: i32 = 1;
    if (y ?= 1 and x ?= 0) {
        result += 10;
    }

    if (x ?= 1 and expensive() ?= 42) {
        result += 100;
    }

    print(f"Result: {result}");

    return result;
}
